# lan
auto {{ lan_iface }}
iface {{ lan_iface }} inet static
    #pre-up iptables-restore < /etc/iptables.up.rules
    address 192.168.0.1
    netmask 255.255.255.0
    dns-nameservers 192.168.0.1
    dns-search deichman.local
    dns-domain deichman.local